        function encodeString(plaintext)
        {
            var encodedtext =3D "";
            for (var i=3D0; i<plaintext.length; i++)
            {
                if (plaintext.charAt(i) =3D=3D " ")
                    encodedtext +=3D "+";
                else
                    encodedtext +=3D plaintext.charAt(i);
            }
            return(escape(encodedtext));
        }
